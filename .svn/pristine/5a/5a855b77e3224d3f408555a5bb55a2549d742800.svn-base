
@{
    Layout = null;
}
@using Model
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>detail</title>
    <link href="~/Scripts/css/table.css" rel="stylesheet" />
</head>
<body>
    <div>
        <table width="100%">
            <tr>
                <td>
                    <font color="black">您正在做的业务是:人力资源--培训管理--调动登记查看</font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="button" class="BUTTON_STYLE1" onclick="javascript:window.history.back();" value="返回">
                </td>
            </tr>
        </table>
        @{ major_change hf = ViewBag.mcobj;}
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr height="21">
                <td class="TD_STYLE1" width="8%">
                    档案编号
                </td>
                <td class="TD_STYLE2" width="10%">
                    @hf.human_id
                </td>
                <td class="TD_STYLE1" width="8%">
                    姓名
                </td>
                <td class="TD_STYLE2" width="10%">
                    @hf.human_name
                </td>
                <td class="TD_STYLE1" width="8%">
                </td>
                <td class="TD_STYLE2" width="10%">
                </td>
                <td class="TD_STYLE1" width="8%">
                </td>
                <td class="TD_STYLE2" width="10%">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1" width="8%">
                    原一级机构
                </td>
                <td class="TD_STYLE2">
                    @hf.first_kind_name
                </td>
                <td class="TD_STYLE1" width="8%">
                    原二级机构
                </td>
                <td class="TD_STYLE2" width="10%">
                    @hf.second_kind_name
                </td>
                <td class="TD_STYLE1" width="8%">
                    原三级机构
                </td>
                <td class="TD_STYLE2" width="10%">
                    @hf.third_kind_name
                </td>
                <td class="TD_STYLE1" width="8%">
                </td>
                <td class="TD_STYLE2" width="10%">
                </td>
            </tr>

            <tr>
                <td class="TD_STYLE1">
                    原职位分类
                </td>
                <td class="TD_STYLE2">
                    @hf.major_kind_name
                </td>
                <td class="TD_STYLE1" width="12%">
                    原职位名称
                </td>
                <td class="TD_STYLE2">
                    @hf.major_name
                </td>
                <td class="TD_STYLE1">
                    原薪酬标准
                </td>
                <td class="TD_STYLE2">
                    @hf.salary_standard_name
                </td>
                <td class="TD_STYLE1">
                    原薪酬总额
                </td>
                <td class="TD_STYLE2">
                    @hf.salary_sum
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    新一级机构名称
                </td>
                <td class="TD_STYLE2">
                    @hf.new_first_kind_name
                </td>
                <td class="TD_STYLE1" width="12%">
                    新二级机构名称
                </td>
                <td class="TD_STYLE2">
                    @hf.new_second_kind_name
                </td>
                <td class="TD_STYLE1" width="12%">
                    新三级机构名称
                </td>
                <td class="TD_STYLE2">
                    @hf.new_third_kind_name
                </td>
                <td class="TD_STYLE1" width="8%">
                </td>
                <td class="TD_STYLE2" width="10%">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    新职位分类
                </td>
                <td class="TD_STYLE2">
                    @hf.new_major_kind_name
                </td>
                <td class="TD_STYLE1" width="12%">
                    新职位名称
                </td>
                <td class="TD_STYLE2">
                    @hf.new_major_name
                </td>
                <td class="TD_STYLE1">
                    新薪酬标准
                </td>
                <td class="TD_STYLE2">
                    @hf.new_salary_standard_name
                </td>
                <td class="TD_STYLE1">
                    新薪酬总额
                </td>
                <td class="TD_STYLE2">
                    @hf.new_salary_sum
                </td>
            </tr>

            <tr>
                <td class="TD_STYLE1">
                    登记人
                </td>
                <td class="TD_STYLE2">
                    @hf.register
                </td>
                <td class="TD_STYLE1">
                    登记时间
                </td>
                <td class="TD_STYLE2">
                    @hf.regist_time
                </td>
                <td class="TD_STYLE1">
                    复核人
                </td>
                <td class="TD_STYLE2">
                    @hf.checker
                </td>
                <td class="TD_STYLE1">
                    复核时间
                </td>
                <td class="TD_STYLE2">
                    @hf.check_reason
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    调动原因
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea name="majorChange.changeReason" rows="6" readonly="readonly" class="TEXTAREA_STYLE1">						
						@hf.change_reason
				    </textarea>
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
